{
  "id": "pg-jdbc",
  "version": "0.0.5",
  "meta": {
    "label": "Pg jdbc",
    "description": "PostgreSQL JDBC dataset connector",
    "author": "ssg",
    "icon": "fas fa-puzzle-piece"
  },

  "parameterSets": [
    {
      "id": "jdbc-basic-auth",
      "label": "PostgreSQL Credentials",
      "description": "User/password for PostgreSQL JDBC connection",
      "params": [
        {
          "name": "user",
          "label": "User",
          "type": "STRING",
          "mandatory": true
        },
        {
          "name": "password",
          "label": "Password",
          "type": "PASSWORD",
          "mandatory": true
        }
      ]
    }
  ],

  "components": [
    {
      "id": "pgjdbc",
      "type": "DATASET",
      "meta": {
        "label": "PostgreSQL(JDBC)",
        "description": "Read PostgreSQL tables through JDBC"
      }
    }
  ]
}