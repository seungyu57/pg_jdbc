{
  "id": "pgjdbc",
  "kind": "DATASET",
  "meta": {
    "label": "PostgreSQL(JDBC)",
    "description": "Read PostgreSQL tables through JDBC (independent connector + external wheel)"
  },
  "params": [
    {
      "name": "jar_path",
      "label": "PostgreSQL JDBC jar path",
      "type": "STRING",
      "mandatory": true,
      "description": "Absolute path to postgresql JDBC jar on DSS server"
    },
    { "name": "host", "label": "Host", "type": "STRING", "mandatory": true },
    { "name": "port", "label": "Port", "type": "INT", "defaultValue": 5432, "mandatory": true },
    { "name": "database", "label": "Database", "type": "STRING", "mandatory": true },
    { "name": "schema", "label": "Schema", "type": "STRING", "defaultValue": "public", "mandatory": true },
    { "name": "table", "label": "Table", "type": "STRING", "mandatory": true },

    {
      "name": "dbCreds",
      "label": "DB Credentials (Preset)",
      "type": "PRESET",
      "parameterSetId": "db-credentials",
      "mandatory": true
    },

    {
      "name": "limit",
      "label": "Row limit",
      "type": "INT",
      "defaultValue": 0,
      "mandatory": false,
      "description": "0 means no limit"
    }
  ],
  "pythonConnectorClass": "connector.PgJdbcConnector"
}