{
  "meta": {
    "label": "PostgreSQL(JDBC)1",
    "description": "Read PostgreSQL tables through JDBC (fixed host/port)",
    "icon": "icon-database"
  },
  "readable": true,
  "writable": false,

  "paramsPythonSetup": "pg_choices.py",

  "params": [
    {
      "name": "pg_credentials",
      "label": "Credentials preset",
      "type": "PRESET",
      "parameterSetId": "pg-credentials",
      "mandatory": true
    },
    {
      "name": "schema",
      "label": "Schema",
      "type": "SELECT",
      "mandatory": true,
      "getChoicesFromPython": true,
      "triggerParameters": ["pg_credentials"]
    },
    {
      "name": "table",
      "label": "Table",
      "type": "SELECT",
      "mandatory": true,
      "getChoicesFromPython": true,
      "triggerParameters": ["pg_credentials", "schema"]
    }
  ]
}